access_id_CA      X509        '/C=IL/O=IUCC/CN=IUCC/emailAddress=ca@mail.iucc.ac.il'
pos_rights        globus      CA:sign
cond_subjects     globus      '"/C=IL/O=IUCC/*"'

access_id_CA      X509        '/C=IL/O=IUCC/CN=IUCC/Email=ca@mail.iucc.ac.il'
pos_rights        globus      CA:sign
cond_subjects     globus      '"/C=IL/O=IUCC/*"'
